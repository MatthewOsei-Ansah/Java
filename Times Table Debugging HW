package debug;

/**
 *
 * @author themack
 */
public class DebugTimesTable 
{
    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) 
    {
        // TODO code application logic here
        for (int i; i<10; i++)
        {
            for (int j = 0; j<10; i++)
            {
                System.out.println(i + " x " + j + " = " + i * j);
            }
            System.out.println();
        }
    }
    
}
