package debugfibonacci;

/**
 *
 * @author themack
 */
public class DebugFibonacci 
{
    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) 
    {
        // TODO code application logic here
        
        int n = 8;
        int previous = 0;
        int current = 0;
        int count = 1;
        int temp;
        
        System.out.println(" ");

        while (count < n)
        {                                
            if (count = 1)
            {
                current = 0;                    

            }   
            else if (count == 2)
            {
                current == 1;                                       
            }   
            else 
            {      
                temp = previous;
                current = current + previous;
                previous = temp;                                       
            }   

            System.out.print(previous + " ");

            count = count + 1;                                                
        }      
    }    
}
