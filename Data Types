public class Main {
    public static void main(String[] args) {
        System.out.println("Hello World"); // Prints Hello World
        System.out.println(3 + 3); // Prints the answer to 3+3
        String name = "Matthew"; // Creates a variable called name and casts it as a string
        int num = 15; // Creates a variable called num and casts it as an integer
        num = 20; // Num now equals 20
        final int number = 25; // Creates a constant called number
        double decimal = 12.5; // Creates a variable called decimal and casts it as a float
        char myLetter = 'D'; // Creates a variable called myLetter and casts it as a character
        boolean myBool = true; // Creates a variable called myBool and casts it as a boolean

    }
}
